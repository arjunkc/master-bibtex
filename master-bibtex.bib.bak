@article{auffinger_universality_2014,
	title = {Universality of chaos and ultrametricity in mixed p-spin models},
	url = {http://arxiv.org/abs/1410.8123},
	abstract = {We prove disorder universality of chaos phenomena and ultrametricity in the mixed p-spin model under mild moment assumptions on the environment. This establishes the long-standing belief among physicists that the Parisi solution in mean-field models is universal. Our results extend to universal properties of other physical observables in the mixed p-spin model as well as in different spin glass models. These include universality of quenched disorder chaos in the Edwards-Anderson (EA) model and quenched concentration for the magnetization in both EA and mixed p-spin models under non-Gaussian environments. In addition, we show quenched self-averaging for the overlap in the random field Ising model under small perturbation of the external field.},
	urldate = {2015-11-13},
	journal = {arXiv:1410.8123 [math]},
	author = {Auffinger, Antonio and Chen, Wei-Kuo},
	month = oct,
	year = {2014},
	note = {arXiv: 1410.8123},
	keywords = {60K35, 82B44, Mathematics - Probability},
	file = {arXiv\:1410.8123 PDF:/home/arjun/.mozilla/firefox/q0zyg42c.default/zotero/storage/456PECVI/Auffinger and Chen - 2014 - Universality of chaos and ultrametricity in mixed .pdf:application/pdf;arXiv.org Snapshot:/home/arjun/.mozilla/firefox/q0zyg42c.default/zotero/storage/IZFINIIK/1410.html:text/html}
}

