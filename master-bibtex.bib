@Article{benjamini_first_2016,
  author   = {Benjamini, Itai and Tessera, Romain},
  title    = {First {Passage} percolation on a hyperbolic graph admits bi-infinite geodesics},
  journal  = {arXiv:1606.02449 [math]},
  year     = {2016},
  month    = jun,
  note     = {arXiv: 1606.02449},
  abstract = {Given an infinite connected graph, a way to randomly perturb its metric is to assign random i.i.d. lengths to the edges. An open question attributed to Furstenberg is whether there exists a two-sided infinite geodesic in first passage percolation on Z{\textasciicircum}2, and more generally on Z{\textasciicircum}n for n{\textgreater}1. Although the answer is generally conjectured to be negative, we give a positive answer for graphs satisfying some negative curvature assumption. Assuming only strict positivity and finite expectation of the random lengths, we prove that if a graph X has bounded degree and contains a Morse geodesic (e.g. is non-elementary Gromov hyperbolic), then almost surely, there exists a bi-infinite geodesic in first passage percolation on X.},
  keywords = {82B43, 51F99, 97K50, Mathematics - Probability},
  pdf      = {arXiv\:1606.02449 PDF:/home/arjun/.mozilla/firefox/29s5a3oy.default-1454961022597/zotero/storage/GGEX98DB/Benjamini and Tessera - 2016 - First Passage percolation on a hyperbolic graph ad.pdf:application/pdf;arXiv.org Snapshot:/home/arjun/.mozilla/firefox/29s5a3oy.default-1454961022597/zotero/storage/U5G3CAVC/1606.html:text/html},
  url      = {http://arxiv.org/abs/1606.02449},
  urldate  = {2016-08-19},
}

